source 'https://rubygems.org'

gem 'jekyll', '~> 3.10.0'

group :jekyll_plugins do
  gem 'jekyll-seo-tag'
  gem 'jekyll-paginate'
  gem 'jekyll-email-protect'
  gem 'jekyll-target-blank'
end

gem "webrick", "~> 1.7"

gem 'wdm', '>= 0.1.0' if Gem.win_platform?

gem 'github-pages', group: :jekyll_plugins

gem 'faraday', '~> 1.0'

